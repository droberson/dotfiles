# Daniel Roberson's .mailcap

application/pdf;	xpdf '%s';


# pictures with feh
image/jpeg;		feh '%s';
image/png;		feh '%s';
image/gif;		feh '%s';

# wav files with aplay
audio/x-wav;		aplay -i '%s'; needsterminal
audio/wav;		aplay -i '%s'; needsterminal

# Word documents w/ antiword
application/msword; antiword %s ; copiousoutput


